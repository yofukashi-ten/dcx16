#information{
  margin-bottom: 60px;
}

#information #head{
  margin-top: 20px;
}

#information .row {
  margin-top: 20px;
}

#information .inner_container {
  margin-left: 10%;
  margin-right: 10%;
}

#information h1 {
  margin-top: 8px;
  margin-bottom: 8px;
  color: #fff;
}

.ggmap {
position: relative;
padding-bottom: 80%;
padding-top: 50px;
height: 0;
overflow: hidden;
}

.ggmap iframe,
.ggmap object,
.ggmap embed {
position: absolute;
top: 0;
left: 0;
width: 100%;
height: 100%;
}

#information table{
  margin-bottom:100px;
}

#information td{
  width: 15%;
  height: 35px;
  color: #fff;
}

#information tr{
  margin-bottom: 30px;
}

@media screen and (min-width: 770px) {
  #information td.thead{
    font-size: 22px;
  }

  #information td.tbody{
    font-size: 16px;
  }
}

@media screen and (min-width: 415px) and (max-width: 769px) {
  #information .thead{
    font-size: 22px;
  }

  #information .tbody{
    font-size: 18px;
  }
}

@media (max-width: 414px) {
  #information .thead {
    font-size: 16px;
  }

  #information .tbody{
    font-size: 14px;
  }
}
