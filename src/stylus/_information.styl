#information{

}

#information h1, h2, h3, h4, h5, p, span, th, td{
  color: #fff;
  margin: 0 0 0 0;
}

#information h2{
  color: #fff;
  margin: 30px 30px 30px 0px;
}

#information
.table>tbody>tr>td,
.table>tbody>tr>th,
.table>tfoot>tr>td,
.table>tfoot>tr>th,
.table>thead>tr>td,
.table>thead>tr>th{
  border-top: none;
  vertical-align: middle;
}

#information th{
  width: 30%;
  font-size: 1em;
  font-weight: bold;
}

#infomation td{
  font-size: 18em;
  text-align: left;
}
